                                                                        <documentation><![CDATA[Datum, ab dem der Unterricht besucht wird (in der Regel der 1. Schultag)]]></documentation>
                                                                      </annotation>
                                                                      <simpleType>
                                                                        <restriction base="string">
                                                                          <pattern value="(0[1-9]|[12][0-9]|3[01])\.(0[1-9]|1[012])\.(19|20)\d\d"></pattern>
                                                                        </restriction>
                                                                      </simpleType>
                                                                    </element>
                                                                    <element name="besucht_bis" minOccurs="0" maxOccurs="1">
                                                                      <annotation>
                                                                        <documentation><![CDATA[Datum, bis zu dem der Unterricht besucht wird (in der Regel der letzte Schultag); leerer Eintrag bedeutet letzter Schultag]]></documentation>
                                                                      </annotation>
                                                                      <simpleType>
                                                                        <restriction base="string">
                                                                          <pattern value="(0[1-9]|[12][0-9]|3[01])\.(0[1-9]|1[012])\.(19|20)\d\d"></pattern>
                                                                        </restriction>
                                                                      </simpleType>
                                                                    </element>
                                                                    <element name="grund_fuer_beendigung" minOccurs="0" maxOccurs="1">
