                                                                        <appinfo>
                                                                          <werteliste plkey="3038">Unterricht Grund für Beendigung</werteliste>
                                                                        </appinfo>
                                                                        <documentation><![CDATA[Grund für eine vorzeitige Beendigung dieses Unterrichtsbesuchs]]></documentation>
                                                                      </annotation>
                                                                      <simpleType>
                                                                        <restriction base="string">
                                                                          <maxLength value="40"></maxLength>
                                                                        </restriction>
                                                                      </simpleType>
                                                                    </element>
                                                                    <element name="unterrichtsart" minOccurs="0" maxOccurs="1">
