<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.asv.bayern.de/import" elementFormDefault="qualified">
  <element name="asv_export">
    <complexType>
      <sequence>
        <element name="schulen" minOccurs="1" maxOccurs="1">
          <annotation>
            <documentation><![CDATA[Alle in diesem Export enthaltenen Schulen]]></documentation>
          </annotation>
          <complexType>
            <sequence>
              <element name="schule" minOccurs="0" maxOccurs="unbounded">
                <annotation>
                  <documentation><![CDATA[Daten einer Schule in einem Schuljahr]]></documentation>
                </annotation>
                <complexType>
                  <sequence>
                    <element name="schulnummer">
                      <annotation>
                        <documentation><![CDATA[Vierstellige, bayernweit eindeutige Nummer der Schule]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <pattern value="\d{4}"></pattern>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="schuljahr">
                      <annotation>
                        <documentation><![CDATA[Schuljahr in der Form 2010/11]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="7"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="schulart">
                      <annotation>
                        <appinfo>
                          <werteliste plkey="2104">Schulart rechtlich</werteliste>
                        </appinfo>
                        <documentation><![CDATA[Schulart, z.B. Gymnasium, Realschule usw.]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="40"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="amtliche_bezeichnung1" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Amtliche Schulbezeichnung 1. Teil]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="240"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="amtliche_bezeichnung2" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Amtliche Schulbezeichnung 2. Teil]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="240"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="amtliche_bezeichnung3" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Amtliche Schulbezeichnung 3. Teil]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="240"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="amtliche_bezeichnung4" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Amtliche Schulbezeichnung 4. Teil]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="240"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="kurzname" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Kurzname der Schule]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="55"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="kurznameZusatz" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Namenszusatz der Schule]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="40"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="dienststellenname" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Der offizielle Name der Schule]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="240"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="bezeichnung_genitiv" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Schulbezeichnung im Genitiv]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="100"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="bezeichnung_akkusativ" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Schulbezeichnung im Akkusativ]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="100"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="bezeichnung_dativ" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Schulbezeichnung im Dativ]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="100"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="zusatzbezeichnung_zeugnis" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Zusatzbezeichnung im Zeugnis (formatiert mit Umbrüchen)]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="240"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="schulname_berufsbildungszentrum1" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Schulbezeichnung Berufsbildungszentrum 1. Teil (nur für BFG, BFS, BOS,BS, BSF,FAK, FOS, FS, WS)]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="100"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="schulname_berufsbildungszentrum2" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Schulbezeichnung Berufsbildungszentrum 2. Teil (nur für BFG, BFS, BOS,BS, BSF,FAK, FOS, FS, WS)]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="100"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="schultraeger" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <appinfo>
                          <werteliste plkey="1029">Schulträger</werteliste>
                        </appinfo>
                        <documentation><![CDATA[Art des Schulträgers]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="40"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="Schulaufwandstraeger" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <appinfo>
                          <werteliste plkey="1028">Schulaufwandsträger</werteliste>
                        </appinfo>
                        <documentation><![CDATA[Art des Schulaufwandsträgers]]></documentation>
                      </annotation>
                      <simpleType>
                        <restriction base="string">
                          <maxLength value="40"></maxLength>
                        </restriction>
                      </simpleType>
                    </element>
                    <element name="anschrift" minOccurs="0" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Daten einer Anschrift]]></documentation>
                      </annotation>
                      <complexType>
                        <sequence>
                          <element name="anschriftstyp" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <appinfo>
                                <werteliste plkey="2010">Anschriftstyp</werteliste>
                              </appinfo>
                              <documentation><![CDATA[Anschriftstyp - ist nur bei bestimmten Anschriften erforderlich]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="40"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="kennung" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <documentation><![CDATA[Falls Schüleranschrift: Kennung des Anschriften-Tabs im Schülermodul]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="30"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="strasse" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <documentation><![CDATA[Straße]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="80"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="nummer" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <documentation><![CDATA[Hausnummer]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="10"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="postleitzahl" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <documentation><![CDATA[Postleitzahl]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="10"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="postfach" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <documentation><![CDATA[Postfach]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="50"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="postleitzahl_postfach" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <documentation><![CDATA[Postleitzahl für das Postfach]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="10"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="ortsbezeichnung" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <documentation><![CDATA[Ortsbezeichnung]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="80"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="ortsbezeichnung_zusatz" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <documentation><![CDATA[Zusatz zur Ortsbezeichnung]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="80"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="ortsteil" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <documentation><![CDATA[Ortsteil]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="80"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="ortsteil_zusatz" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <documentation><![CDATA[Zusatz zum Ortsteil]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="80"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="anredetext" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <documentation><![CDATA[Anredetext bei Anschreiben, z. B. 'Sehr geehrter Herr Dr. Werner Huber']]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="128"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="anschrifttext" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <documentation><![CDATA[Text bei Anschriften Teil der Adresse 'oberhalb' von Straße und Ort]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="128"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="staat" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <appinfo>
                                <werteliste plkey="2118">Staat</werteliste>
                              </appinfo>
                              <documentation><![CDATA[Staat]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="40"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="gemeinde" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <appinfo>
                                <werteliste plkey="xxxx">Gemeinde</werteliste>
                              </appinfo>
                              <documentation><![CDATA[Gemeinde]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <maxLength value="40"></maxLength>
                              </restriction>
                            </simpleType>
                          </element>
                          <element name="kommunikationsdaten" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <documentation><![CDATA[Kommunikationsdaten (z.B. Telefon,  E-Mail,  Fax,  URL]]></documentation>
                            </annotation>
                            <complexType>
                              <sequence>
                                <element name="kommunikation" minOccurs="0" maxOccurs="unbounded">
                                  <annotation>
                                    <documentation><![CDATA[Kommunikationsdatensatz, z.B. Telefonnummer, Faxnummer, E-Mail, URL]]></documentation>
                                  </annotation>
                                  <complexType>
                                    <sequence>
                                      <element name="typ">
                                        <annotation>
                                          <appinfo>
                                            <werteliste plkey="2009">Kommunikationsanschluss</werteliste>
                                          </appinfo>
                                          <documentation><![CDATA[Art der Kommunikation, z.B. Telefon, E-Mail, Fax]]></documentation>
                                        </annotation>
                                        <simpleType>
                                          <restriction base="string">
                                            <maxLength value="40"></maxLength>
                                          </restriction>
                                        </simpleType>
                                      </element>
                                      <element name="nummer_adresse" minOccurs="0" maxOccurs="1">
                                        <annotation>
                                          <documentation><![CDATA[Nummer/Adresse]]></documentation>
                                        </annotation>
                                        <simpleType>
                                          <restriction base="string">
                                            <maxLength value="100"></maxLength>
                                          </restriction>
                                        </simpleType>
                                      </element>
                                      <element name="bemerkung" minOccurs="0" maxOccurs="1">
                                        <annotation>
                                          <documentation><![CDATA[Bemerkung]]></documentation>
                                        </annotation>
                                        <simpleType>
                                          <restriction base="string">
                                            <maxLength value="100"></maxLength>
                                          </restriction>
                                        </simpleType>
                                      </element>
                                    </sequence>
                                  </complexType>
                                </element>
                              </sequence>
                            </complexType>
                          </element>
                          <element name="im_verteiler_schriftverkehr" minOccurs="0" maxOccurs="1">
                            <annotation>
                              <documentation><![CDATA[Anschrift im Verteiler für den Schriftverkehr (0 = nicht im Verteiler [Default], 1 = im Verteiler)]]></documentation>
                            </annotation>
                            <simpleType>
                              <restriction base="string">
                                <pattern value="(true|false)"></pattern>
                              </restriction>
                            </simpleType>
                          </element>
                        </sequence>
                      </complexType>
                    </element>
                    <element name="klassen" minOccurs="1" maxOccurs="1">
                      <annotation>
                        <documentation><![CDATA[Alle Klassen der Schule]]></documentation>
                      </annotation>
                      <complexType>
                        <sequence>
                          <element name="klasse" minOccurs="0" maxOccurs="unbounded">
                            <annotation>
                              <documentation><![CDATA[Daten einer Klasse in einem Schuljahr]]></documentation>
                            </annotation>
                            <complexType>
                              <sequence>
                                <element name="xml_id">
                                  <annotation>
                                    <documentation><![CDATA[Eindeutige Id der Klasse innerhalb der XML-Datei]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="integer">
                                      <minInclusive value="0"></minInclusive>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="klassenname">
                                  <annotation>
                                    <documentation><![CDATA[Klassenname]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="string">
                                      <maxLength value="32"></maxLength>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="klassenname_lang">
                                  <annotation>
                                    <documentation><![CDATA[Langform des Klassennamens]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="string">
                                      <maxLength value="40"></maxLength>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="klassenname_naechstes_schuljahr" minOccurs="0" maxOccurs="1">
                                  <annotation>
                                    <documentation><![CDATA[Klassenname für das nächste Schuljahr]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="string">
                                      <maxLength value="32"></maxLength>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="klassenname_zeugnis" minOccurs="0" maxOccurs="1">
                                  <annotation>
                                    <documentation><![CDATA[Klassenbezeichnung für das Zeugnis]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="string">
                                      <maxLength value="32"></maxLength>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="klassenart" minOccurs="0" maxOccurs="1">
                                  <annotation>
                                    <appinfo>
                                      <werteliste plkey="2075">Klassenart</werteliste>
                                    </appinfo>
                                    <documentation><![CDATA[Klassenart]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="string">
                                      <maxLength value="40"></maxLength>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="organisationsform" minOccurs="0" maxOccurs="1">
                                  <annotation>
                                    <appinfo>
                                      <werteliste plkey="2151">Organisationsform</werteliste>
                                    </appinfo>
                                    <documentation><![CDATA[Organisationsform der Klasse]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="string">
                                      <maxLength value="40"></maxLength>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="klassenbesonderheit" minOccurs="0" maxOccurs="1">
                                  <annotation>
                                    <appinfo>
                                      <werteliste plkey="2150">KlassenBesonderheit</werteliste>
                                    </appinfo>
                                    <documentation><![CDATA[Besonderheit der Klasse (allgemeinbildende Schulen)]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="string">
                                      <maxLength value="40"></maxLength>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="foerderschwerpunkt" minOccurs="0" maxOccurs="1">
                                  <annotation>
                                    <appinfo>
                                      <werteliste plkey="1013">Förderschwerpunkt</werteliste>
                                    </appinfo>
                                    <documentation><![CDATA[Förderschwerpunkt (VSF, RSF)]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="string">
                                      <maxLength value="40"></maxLength>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="ausgelagert" minOccurs="0" maxOccurs="1">
                                  <annotation>
                                    <documentation><![CDATA[Kennzeichnung, ob Klasse ausgelagert (0 = falsch [Default], 1 = wahr - ausgelagert)]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="integer">
                                      <maxInclusive value="1"></maxInclusive>
                                      <minInclusive value="0"></minInclusive>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="ausgelagert_ort" minOccurs="0" maxOccurs="1">
                                  <annotation>
                                    <documentation><![CDATA[Ort der Auslagerung der Klasse]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="string">
                                      <maxLength value="100"></maxLength>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="aussenklasse" minOccurs="0" maxOccurs="1">
                                  <annotation>
                                    <documentation><![CDATA[Kennzeichnung, ob Klasse eine Außenklasse ist (0 = Nein, 1 = Ja) Bayern: Nach Art. 30 Abs. 1 Satz 4 BayEUG (VS, VSF)]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="integer">
                                      <maxInclusive value="1"></maxInclusive>
                                      <minInclusive value="0"></minInclusive>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="unterrichtstage" minOccurs="0" maxOccurs="1">
                                  <annotation>
                                    <documentation><![CDATA[Unterrichtstag(e) (BS, BSF) Es können auch mehrere Unterrichtstage erfasst werden, z. B. MO/MI]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="string">
                                      <maxLength value="20"></maxLength>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="berufsfeld" minOccurs="0" maxOccurs="1">
                                  <annotation>
                                    <appinfo>
                                      <werteliste plkey="2026">Berufsfeld</werteliste>
                                    </appinfo>
                                    <documentation><![CDATA[Berufsfeld der Klasse]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="string">
                                      <maxLength value="40"></maxLength>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="fachklassengliederungsnummer" minOccurs="0" maxOccurs="1">
                                  <annotation>
                                    <appinfo>
                                      <werteliste plkey="2059">Fachklassengliederungsnummer</werteliste>
                                    </appinfo>
                                    <documentation><![CDATA[Fachklassennummer beinhaltet die Fachklassengleiderungsnummer]]></documentation>
                                  </annotation>
                                  <simpleType>
                                    <restriction base="string">
                                      <maxLength value="40"></maxLength>
                                    </restriction>
                                  </simpleType>
                                </element>
                                <element name="anschrift" minOccurs="0" maxOccurs="1">
                                  <annotation>
                                    <documentation><![CDATA[Daten einer Anschrift]]></documentation>
                                  </annotation>
                                  <complexType>
                                    <sequence>
                                      <element name="anschriftstyp" minOccurs="0" maxOccurs="1">
                                        <annotation>
                                          <appinfo>
                                            <werteliste plkey="2010">Anschriftstyp</werteliste>
                                          </appinfo>
                                          <documentation><![CDATA[Anschriftstyp - ist nur bei bestimmten Anschriften erforderlich]]></documentation>
                                        </annotation>
                                        <simpleType>
                                          <restriction base="string">
                                            <maxLength value="40"></maxLength>
                                          </restriction>
                                        </simpleType>
                                      </element>
                                      <element name="kennung" minOccurs="0" maxOccurs="1">
                                        <annotation>
                                          <documentation><![CDATA[Falls Schüleranschrift: Kennung des Anschriften-Tabs im Schülermodul]]></documentation>
                                        </annotation>
                                        <simpleType>
                                          <restriction base="string">
                                            <maxLength value="30"></maxLength>
                                          </restriction>
                                        </simpleType>
                                      </element>
                                      <element name="strasse" minOccurs="0" maxOccurs="1">
                                        <annotation>
                                          <documentation><![CDATA[Straße]]></documentation>
                                        </annotation>
                                        <simpleType>
                                          <restriction base="string">
                                            <maxLength value="80"></maxLength>
                                          </restriction>
                                        </simpleType>
                                      </element>
                                      <element name="nummer" minOccurs="0" maxOccurs="1">
                                        <annotation>
                                          <documentation><![CDATA[Hausnummer]]></documentation>
                                        </annotation>
                                        <simpleType>
                                          <restriction base="string">
                                            <maxLength value="10"></maxLength>
                                          </restriction>
                                        </simpleType>
                                      </element>
